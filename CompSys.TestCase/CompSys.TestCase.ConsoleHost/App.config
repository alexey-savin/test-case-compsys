<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
  </configSections>
  <connectionStrings>
    <add name="TestCaseData"
      connectionString="Server=(local);Database=TestCaseData;Integrated Security=SSPI" />
  </connectionStrings>
  <system.serviceModel>
    <services>
      <service name="CompSys.TestCase.WCFService.CompSysService">
        <endpoint address="" binding="basicHttpBinding" contract="CompSys.TestCase.WCFService.ICompSysService"/>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/CompSysService" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="True" httpsGetEnabled="True"/>
          <serviceDebug includeExceptionDetailInFaults="True" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>